#include <stdio.h>
#include <string.h>
#include <sys/wait.h>
#include <unistd.h>
int main() {
  pid_t pid = fork();
  char str[250];
  if (pid == 0) {

    printf("Enter programs to run \n ");
    fflush(stdout);
    fgets(str, sizeof(str), stdin);
    printf("%s", str);
  }else{
    wait(NULL);
  }

  return 0;
}
