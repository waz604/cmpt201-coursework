#include <pthread.h>
#include <stdio.h>
#include <unistd.h>
#include <string.h>

void *threadfunc(void *arg) {
  char *message = (char *)arg;
  printf("hi \n");
  printf("message %s\n", message);
  return (void *)strlen(message);
}

int main(void) {
  pthread_t p;
  pthread_create(&p, NULL, threadfunc, "hiiii");
  void* ans;
  pthread_join(p, &ans);
 long long len = (long long) ans;
  printf("%lld\n",len);
}
