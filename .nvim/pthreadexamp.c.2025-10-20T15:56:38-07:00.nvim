#include <pthread.h>
#include <stdio.h>
#include <unistd.h>
void *threadfunc(void *arg) {
  char* message = (char*) arg;
  printf("hi \n");
  printf("message %s\n", message);
  return NULL;
}

int main(void) {
  pthread_t p;
  pthread_create(&p, NULL, threadfunc, "hiiii");
  pthread_join(p, NULL);
}
